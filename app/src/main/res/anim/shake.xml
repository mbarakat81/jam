        android:startOffset="100"
        android:fromXDelta="0%p"
        android:toXDelta="-1%p"
        android:duration="25" />
    <translate
        android:startOffset="100"
        android:fromXDelta="0%p"
        android:toXDelta="1%p"
        android:duration="50" />
    <translate
        android:startOffset="100"
        android:fromXDelta="0%p"
        android:toXDelta="-1%p"
        android:duration="25" />
    <translate
        android:startOffset="100"
        android:fromXDelta="0%p"
        android:toXDelta="1%p"
        android:duration="50" />
    <translate
        android:startOffset="100"
        android:fromXDelta="0%p"
        android:toXDelta="-1%p"
        android:duration="25" />

</set>